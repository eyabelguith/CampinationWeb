<body class="login" style="background-image: url('assets/img/home_img/mountain.jpg');">
	<div class="pre-loader">
		<div class="loading-img"></div>
	</div>
	
	<div class="form-box">
		<div class="form-head">
			<div class="mold-logo">
				
				<div class="logo"></div>
				<div class="logo-txt">Campination</div>
			</div>
			<hr>
			<div class="txt">ajout nouvelle destination</div>
		</div>
		<div class="form-body">
            
           {{form_start(FormA, {'attr':{'novalidate':'novalidate'}})}}  
				<div class="form-group">
					{{form_label(FormA.nom, "libell√© destination")}}
					<div class="input-group">
						
						{{form_widget(FormA.nom)}}
                        {{form_errors(FormA.nom)}}
					</div>
				</div>
				<div class="form-group">
					{{form_label(FormA.gouvernorat)}}
                    
					<div class="input-group">
						
						{{form_widget(FormA.gouvernorat)}}
                        {{form_errors(FormA.gouvernorat)}}
					</div>
				</div>
				<div class="form-group">
					{{form_label(FormA.type,"type de destination")}}
					<div class="input-group">
						
						{{form_widget(FormA.type)}}
                        {{form_errors(FormA.type)}}
					</div>
				</div>
				
				<br>
				{{form_widget(FormA.save)}} 
			{{form_end(FormA)}}



            
		</div>
	</div>
	
